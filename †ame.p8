--init
function _init()
hero = {}
col = {} -- colide position
hero.x = 4*8
hero.y = 24
hero.x_dir = 1 --1 >= dir >= -1
hero.y_dir = 0 --1 >= dir >= -1
hero.flip = false
hero.map_x = 0 -- pos on map
hero.map_y = 0
hero.sx = 1 -- speed
hero.sy = 1 -- speed
hero.on_flore = false
h_g = 0 -- hero gravity get() 
end


-->8
--update
function _update()
key_control()
camera(hero.x-64,hero.y-64)
end


-->8
--draw
function _draw()
cls()
map()
print(hero.on_flore,hero.x-10,hero.y-10)
print(h_g,hero.x-14,hero.y-6)

_draw_hero()
end

function _draw_hero()
sspr (0,0,16,16,
hero.x,hero.y,16,16,
hero.flip,false)
end
