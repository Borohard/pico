--movement
function key_control()

if(hero.sx == 0) then
hero.x_dir = -1*hero.x_dir
check_flip()
end
hero.sx = 1


if(hero.sy == 0) then
 h_g = 1
end

if(btnp(2)
 and hero.sy == 0 
 and hero.on_flore) then
 h_g = -4
end
 
h_g += 0.2
hero.y_dir = sgn(h_g)
hero.sy = abs(h_g)
 
hero_move(move)
end

function hero_move(pos)
xd =flr((hero.x_dir+1)/2)
yd =flr((hero.y_dir+1)/2)

col = dist_to_col(hero.x_dir,0)
if (col.x ~= nil and 
col.x-(8*xd) <= hero.sx) then
 hero.sx = 0
 end
 
col = dist_to_col(0,hero.y_dir)
if (col.y ~= nil and 
    col.y-(8*yd) <= hero.sy) then
 check_flore()
 hero.sy = 0
 end
 
hero.x += hero.x_dir*hero.sx
hero.y += hero.y_dir*hero.sy
hero.map_x = 8*flr((hero.x+7)/8)
hero.map_y = 8*flr((hero.y+7)/8)
end

--only hero
function check_flore()
 hero.on_flore = false
 if hero.y_dir == 1 then
  hero.on_flore = true
  end
end

--only hero
function check_flip()
if hero.x_dir == -1 then
 hero.flip = true
elseif hero.x_dir == 1 then
 hero.flip = false
 end
end
-->8
--colide
function dist_to_col(x,y)
local result = {}
local c = {} -- colide position

c.x= hero.map_x +(x*8)
c.y= hero.map_y +(y*8)

if is_b(c.x,c.y) then
 result.x= abs(c.x-hero.x)-7
 result.y= abs(c.y-hero.y)-7
elseif is_b(c.x,c.y+8) then
 result.x= abs(c.x-hero.x)-7
 result.y= abs(c.y-hero.y+8)-7
elseif is_b(c.x+8,c.y) then
 result.x= abs(c.x-hero.x+8)-7
 result.y= abs(c.y-hero.y)-7
elseif is_b(c.x+8,c.y+8) then
 result.x= abs(c.x-hero.x+8)-7
 result.y= abs(c.y-hero.y+8)-7
 end
 
 return result
end

function is_b(x,y) -- is block

map_n = mget(x/8,y/8)
return fget(map_n,0)
end
